{
  "name": "myapp",
  "version": "1.0.0",
  "description": "myapp API",
  "config": {
    "projectName": "myapp"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf node_modules",
    "compoze:sqs": "compoze lambda.listener -t sqs",
    "compoze:http": "compoze lambda.api.http",
    "compoze:sqswriter": "compoze lambda.writer -t sqs",
    "compoze:eventbridge": "compoze lambda.eventbridge.listener",
    "compoze:eventbridgewriter": "compoze lambda.eventbus.writer",
    "compoze:eventbridgecronwriter": "compoze lambda.eventbus.cron",
    "compoze:dynamo": "compoze lambda.dynamodb",
    "compoze:environment": "compoze environment",
    "deploy": ".compoze/scripts/deploy.sh $npm_package_config_projectName",
    "test": "jest --config=jest.config.js",
    "postbuild": "echo \"moving node_modules to deploy\" && cp -r node_modules deploy",
    "precompoze:sqs": "npm ci",
    "precompoze:environment": "npm ci",
    "precompoze:http": "npm ci",
    "precompoze:sqswriter": "npm ci",
    "precompoze:eventbridge": "npm ci",
    "precompoze:eventbridgewriter": "npm ci",
    "precompoze:eventbridgecronwriter": "npm ci",
    "precompoze:dynamo": "npm ci"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.29.0",
    "moment": "^2.29.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@compoze/compoze-framework": "^0.16.13",
    "@types/aws-lambda": "^8.10.70",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.20",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.815.0",
    "dotenv": "^8.2.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
