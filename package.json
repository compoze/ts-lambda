{
  "name": "myapp",
  "version": "1.0.0",
  "description": "myapp API",
  "config": {
    "projectName": "myapp",
    "region": "REGION_REPLACEMENT"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf node_modules",
    "deploy": ".compoze/scripts/deploy.sh $npm_package_config_projectName $npm_package_config_region",
    "start:local": "sam local start-api",
    "test": "jest --config=jest.config.js",
    "postbuild": "echo \"moving node_modules to deploy\" && cp -r node_modules deploy"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.29.0",
    "moment": "^2.29.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.70",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.20",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.815.0",
    "dotenv": "^8.2.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
